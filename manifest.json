{
  "manifest_version": 3,
  "name": "Shift Calendar View",
  "version": "1.0",
  "description": "Transforms shift table into a calendar view",
  "icons": {
    "48": "icons/icon.png"
  },
  "host_permissions": [
    "https://edm.meduniwien.ac.at/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://edm.meduniwien.ac.at/dienstplan.php*"],
      "js": ["scripts/shiftCalendar.js", "scripts/controllers.js","scripts/shiftCalendarPlugin.js"],
      "css": ["styles/purpleCalendarTheme.css"]
    }
  ]
} 